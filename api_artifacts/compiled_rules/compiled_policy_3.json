  "canonical_rules": [
    {
      "id": "utah_medicaid_mri_knee_without_contrast_eligible_diagnosis",
      "description": "Member must have a documented diagnosis from one of the eligible categories",
      "logic": "any",
      "conditions": [
        {
          "field": "icd10_code",
          "operator": "in",
          "value": [
            "M23.20x",
            "M23.21x",
            "M23.22x",
            "M23.30x",
            "S83.5xx",
            "S83.6xx",
            "S83.9xx",
            "M23.60x",
            "M23.61x",
            "M23.62x",
            "M93.2xx",
            "M25.561",
            "M25.562",
            "M25.569",
            "M17.0x",
            "M17.1x",
            "M17.2x",
            "M17.3x"
          ]
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_clinical_findings",
      "description": "At least one of the clinical findings must be documented on physical examination within 30 days prior to the PA request",
      "logic": "any",
      "conditions": [
        {
          "field": "mcmurrays_test_result",
          "operator": "eq",
          "value": true
        },
        {
          "field": "thessaly_test_result",
          "operator": "eq",
          "value": true
        },
        {
          "field": "joint_line_tenderness",
          "operator": "eq",
          "value": true
        },
        {
          "field": "persistent_joint_effusion",
          "operator": "eq",
          "value": true
        },
        {
          "field": "lachman_test_result",
          "operator": "eq",
          "value": true
        },
        {
          "field": "anterior_posterior_drawer_test_result",
          "operator": "eq",
          "value": true
        },
        {
          "field": "instability_on_varus_or_valgus_stress_testing",
          "operator": "eq",
          "value": true
        },
        {
          "field": "restricted_range_of_motion",
          "operator": "eq",
          "value": true
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_prior_imaging",
      "description": "Weight-bearing radiographs of the affected knee must be completed within 60 calendar days prior to the date of the MRI prior authorization request",
      "logic": "all",
      "conditions": [
        {
          "field": "weight_bearing_radiographs_completed",
          "operator": "eq",
          "value": true
        },
        {
          "field": "radiograph_report_available",
          "operator": "eq",
          "value": true
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_conservative_treatment",
      "description": "Member must have completed a minimum of six weeks of conservative therapy",
      "logic": "count_gte",
      "threshold": 2,
      "conditions": [
        {
          "field": "physical_therapy_completed",
          "operator": "eq",
          "value": true
        },
        {
          "field": "nsaids_or_oral_analgesics_trial",
          "operator": "eq",
          "value": true
        },
        {
          "field": "activity_modification_and_home_exercise_program",
          "operator": "eq",
          "value": true
        },
        {
          "field": "bracing_orthotics_or_assistive_devices",
          "operator": "eq",
          "value": true
        },
        {
          "field": "intra_articular_injection",
          "operator": "eq",
          "value": true
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_special_population_requirements",
      "description": "Special population requirements",
      "logic": "all",
      "conditions": [
        {
          "field": "pediatric_orthopedic_evaluation",
          "operator": "eq",
          "value": true
        },
        {
          "field": "surgical_candidacy_assessment",
          "operator": "eq",
          "value": true
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_exception_acute_traumatic_injury",
      "description": "Acute traumatic injury exception",
      "logic": "any",
      "conditions": [
        {
          "field": "acute_traumatic_injury",
          "operator": "eq",
          "value": true
        },
        {
          "field": "suspected_complete_rupture_of_major_ligament",
          "operator": "eq",
          "value": true
        },
        {
          "field": "clinical_findings_consistent_with_displaced_meniscal_tear",
          "operator": "eq",
          "value": true
        }
      ],
      "exception_pathway": true,
      "overrides": [
        "utah_medicaid_mri_knee_without_contrast_conservative_treatment"
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_exception_post_operative",
      "description": "Post-operative exception",
      "logic": "all",
      "conditions": [
        {
          "field": "post_operative_evaluation_required",
          "operator": "eq",
          "value": true
        },
