    {
      "id": "utah_medicaid_mri_knee_without_contrast_eligible_diagnosis",
      "description": "Member must have a documented diagnosis from one of the following categories",
      "logic": "any",
      "conditions": [
        {
          "field": "icd10_diagnosis_code",
          "operator": "in",
          "value": [
            "M23.20x",
            "M23.21x",
            "M23.22x",
            "M23.30x",
            "S83.5xx",
            "S83.6xx",
            "S83.9xx",
            "M23.60x",
            "M23.61x",
            "M23.62x",
            "M93.2xx",
            "M25.561",
            "M25.562",
            "M25.569",
            "M17.0x",
            "M17.1x",
            "M17.2x",
            "M17.3x"
          ]
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_clinical_findings",
      "description": "At least one of the following clinical findings must be documented on physical examination within 30 days prior to the PA request",
      "logic": "any",
      "conditions": [
        {
          "field": "positive_mcmurrays_test",
          "operator": "eq",
          "value": true
        },
        {
          "field": "positive_thessaly_test",
          "operator": "eq",
          "value": true
        },
        {
          "field": "joint_line_tenderness_with_mechanical_symptoms",
          "operator": "eq",
          "value": true
        },
        {
          "field": "persistent_joint_effusion",
          "operator": "eq",
          "value": true
        },
        {
          "field": "positive_lachman_test",
          "operator": "eq",
          "value": true
        },
        {
          "field": "positive_anterior_posterior_drawer_test",
          "operator": "eq",
          "value": true
        },
        {
          "field": "instability_on_varus_or_valgus_stress_testing",
          "operator": "eq",
          "value": true
        },
        {
          "field": "restricted_range_of_motion_with_mechanical_block",
          "operator": "eq",
          "value": true
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_prior_imaging",
      "description": "Weight-bearing radiographs (X-rays) of the affected knee must be completed within 60 calendar days prior to the date of the MRI prior authorization request",
      "logic": "all",
      "conditions": [
        {
          "field": "weight_bearing_radiographs_completed",
          "operator": "eq",
          "value": true
        },
        {
          "field": "radiograph_report_available",
          "operator": "eq",
          "value": true
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_conservative_treatment",
      "description": "Member must have completed a minimum of six weeks of conservative therapy",
      "logic": "count_gte",
      "threshold": 2,
      "conditions": [
        {
          "field": "physical_therapy_completed",
          "operator": "eq",
          "value": true
        },
        {
          "field": "nsaids_or_oral_analgesics_trial",
          "operator": "eq",
          "value": true
        },
        {
          "field": "activity_modification_and_home_exercise_program",
          "operator": "eq",
          "value": true
        },
        {
          "field": "bracing_orthotics_or_assistive_devices",
          "operator": "eq",
          "value": true
        },
        {
          "field": "intra_articular_injection",
          "operator": "eq",
          "value": true
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_special_population_under_18",
      "description": "Pediatric orthopedic evaluation by a board-certified or board-eligible pediatric orthopedic surgeon must be documented prior to PA approval",
      "logic": "all",
      "conditions": [
        {
          "field": "pediatric_orthopedic_evaluation",
          "operator": "eq",
          "value": true
        },
        {
          "field": "growth_plate_assessment",
          "operator": "eq",
          "value": true
        }
      ]
    },
    {
      "id": "utah_medicaid_mri_knee_without_contrast_special_population_over_65",
      "description": "Documentation of functional impact on activities of daily living required",
      "logic": "all",
      "conditions": [
        {
          "field": "functional_impact_on_activities_of_daily_living",
          "operator": "eq",
          "value": true
